{
	"name": "goScore (Chrome Extension)",
	"description": "This tool check security score before open site.",
	"version": "0.1",
	"background" : {
		"scripts" : ["background.js"]
	},
	"browser_action": {
			"default_popup": "popup.html"
	},
	"manifest_version": 2,
	"permissions":[
		"tabs",
	    "storage",
		"http://*/*",
		"https://*/*",
		"contextMenus"
	]
}

